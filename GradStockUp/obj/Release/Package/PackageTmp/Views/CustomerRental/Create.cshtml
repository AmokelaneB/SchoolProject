@model GradStockUp.Models.CustomerRental

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>Run</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/1.10.16/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/Scripts/select2.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.select.min.js" type="text/javascript"></script>
    <link href="~/Content/DataTables/css/select.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/css/select2.min.css" rel="stylesheet" />
    <script src="~/Scripts/Sidebar-Menu.js" type="text/javascript"></script>
    <link rel="stylesheet" href="~/Content/css/Sidebar-Menu-1.css" />
    <link rel="stylesheet" href="~/Content/css/Sidebar-Menu.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.0.2/js/toastr.min.js"></script>
    <style>
        table.outlined {
            border-collapse: collapse;
        }

            table.outlined tr {
                border: 1px solid goldenrod;
            }

        .dropdown {
            position: relative;
            display: inline-block;
        }

        .dropdown-content {
            display: none;
            position: absolute;
            background-color: #f9f9f9;
            min-width: 160px;
            box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
            padding: 12px 16px;
            z-index: 1;
        }

        .dropdown:hover .dropdown-content {
            display: block;
        }
    </style>
    <script>
        $(document).ready(function () {

            toastr.options = {
                "debug": false,
                "positionClass": "toast-top-full-width",
                "onclick": null,
                "fadeIn": 300,
                "fadeOut": 1000,
                "timeOut": 5000,
                "extendedTimeOut": 1000
            }

            $('select').select2({ width: '100%' });
            $.ajax({
                type: "GET",
                url: "/CustomerRental/getEstablishment",
                data: "{}",
                success: function (data) {


                    var s = '< option value ="0" selected>Select Establishment...  </option > ';
                    for (var i = 0; i < data.length; i++) {

                        s += '<option   value = "' + data[i].EstaID + '" > ' + data[i].Description + '</option > ';
                    }

                    $("#Establishment").append(s);


                }
            });

            $('#Establishment').on("change", function () {

                $('#Institutes').attr("Disabled", false);
                $.ajax({
                    type: "GET",
                    url: "/CustomerRental/getInstitution",
                    data: "{}",
                    success: function (data) {
                        var s = '';
                        $("#Institutes").empty();
                        $('#Institutes').attr("Disabled", true);
                        $("#Faculty").empty();
                        $('#Faculty').attr("Disabled", true);
                        $("#QualType").empty();
                        $('#QualType').attr("Disabled", true);
                        s = '< option value ="ff">  </option > ';
                        for (var i = 0; i < data.length; i++) {


                            if (data[i].EstaID == $("#Establishment").val())
                                s += '<option  style="background-color: black" value = "' + data[i].InstID + '" > ' + data[i].description + '</option > ';
                        }
                        $("#Institutes").append(s);
                        $('#Institutes').attr("Disabled", false);

                    }
                });

            });

            $('#Institutes').on("change", function () {
                $("#QualType").empty();
                $('#Faculty').attr("Disabled", false);
                $.ajax({
                    type: "GET",
                    url: "/CustomerRental/getFaculties",
                    data: "{}",
                    success: function (data) {

                        if (data == "NoItem") {
                            $("#QualType").attr("Disabled", false);
                            $("#QualType").empty();
                            $.ajax({
                                type: "GET",
                                url: "/CustomerRental/getQualification",
                                data: "{}",
                                success: function (data) {
                                    var s;
                                    var s = '< option value ="ff" selected>Select Qualification...  </option > ';
                                    for (var i = 0; i < data.length; i++) {

                                        if (data[i].InstID == $("#Institutes").val()) {
                                            s += '<option  style="background-color: black" value = "' + data[i].QualID + '" > ' + data[i].description + '</option > ';
                                        }

                                    }
                                    $("#QualType").append(s);
                                    $('#QualType').attr("Disabled", false);

                                }
                            });
                        }
                        else {
                            var s = '< option value ="ff" selected>Select Faculty...  </option > ';
                            var count = 0;
                            for (var i = 0; i < data.length; i++) {
                                if (data[i].InstID == $("#Institutes").val())
                                {
                                    s += '<option  style="background-color: black" value = "' + data[i].FacID + '" > ' + data[i].description + '</option > ';
                                    count++;
                                }
                            }

                            if (count == 0) {
                                $("#FacultyDiv").hide();
                                $("#ColorDiv").show();
                                $("#ColorDiv").attr("hidden", false)
                                $("#QualType").attr("Disabled", false);
                                $("#QLY").hide();
                                $("#ColourItems").empty();
                                $.ajax({
                                    type: "GET",
                                    url: "/CustomerRental/getColours",
                                    data: "{}",
                                    success: function (data) {
                                        var s;
                                        var s = '< option value ="ff" selected>Select Qualification...  </option > ';
                                        for (var i = 0; i < data.length; i++) {
                                                s += '<option  style="background-color: black" value = "' + data[i].EstaID + '" > ' + data[i].Description + '</option > ';
                                        }
                                        $("#ColourItems").append(s);
                                        $('#ColourItems').attr("Disabled", false);
                                        $("#DateFunction").attr("Disabled", false);
                                        var todaysDate = new Date(); // Gets today's date
                                        // Max date attribute is in "YYYY-MM-DD".  Need to format today's date accordingly

                                        var year = todaysDate.getFullYear();                        // YYYY

                                        var month = ("0" + (todaysDate.getMonth() + 1)).slice(-2);  // MM

                                        var day = ("0" + todaysDate.getDate()).slice(-2);           // DD
                                        var minDate = (year + "-" + month + "-" + day);
                                        $('#DateFunction').attr('min', minDate);
                                    }
                                });
                            }
                            else {
                                $("#FacultyDiv").show();
                                $("#Faculty").append('< option value ="0" selected>Select Faculty...  </option > ');
                                $("#Faculty").append(s);
                                $('#Faculty').attr("Disabled", false);
                                $("#ColorDiv").hide();
                                $("#ColorDiv").attr("hidden", true)
                                $("#QLY").show();
                            }
                        }
                    }
                });
            });
            $('#Faculty').change(function () {
                $("#QualType").attr("Disabled", false);
                $("#QualType").empty();
                $.ajax({
                    type: "GET",
                    url: "/CustomerRental/getQualification",
                    data: "{}",
                    success: function (data) {
                        var s;
                        var s = '< option value ="ff" selected>Select Qualification...  </option > ';
                        for (var i = 0; i < data.length; i++) {

                            if (data[i].InstID == $("#Institutes").val() && data[i].FacID == $('#Faculty').val()) {
                                s += '<option  style="background-color: black" value = "' + data[i].QualID + '" > ' + data[i].description + '</option > ';
                            }

                        }
                        $("#QualType").append(s);
                        $('#QualType').attr("Disabled", false);

                    }
                });

            });



            $('#QualType').change(function () {
                $("#DateFunction").attr("Disabled", false);
                var todaysDate = new Date(); // Gets today's date
                // Max date attribute is in "YYYY-MM-DD".  Need to format today's date accordingly

                var year = todaysDate.getFullYear();                        // YYYY

                var month = ("0" + (todaysDate.getMonth() + 1)).slice(-2);  // MM

                var day = ("0" + todaysDate.getDate()).slice(-2);           // DD
                var minDate = (year + "-" + month + "-" + day);
                $('#DateFunction').attr('min', minDate);
            });

            $('#DateFunction').change(function () {
                $("#HeadCircum").attr("Disabled", false);
                $.ajax({
                    type: "GET",
                    url: "/CustomerRental/getHeadCircum",
                    data: "{}",
                    success: function (data) {
                        var s;
                        var s = '< option value ="ff" selected>Select Size...  </option > ';
                        for (var i = 0; i < data.length; i++) {
                            if (data[i].stockTypeID == 1)
                                s += '<option value = "' + data[i].ID + '" > ' + data[i].SizeDesc + '</option > ';
                        }
                        $("#HeadCircum").append(s);

                    }
                });

            });
            $('#DateFunction').change(function () {

                $("#Out *").attr("disabled", false);
            

            });
            $("#StockType").change(function () {
                $("#PriceItem").attr("value", "");
                $.ajax({
                    type: "GET",
                    url: "/CustomerRental/getHeadCircum",
                    data: "{}",
                    success: function (data) {
                        var s = '< option value ="ff" selected>Select Size...  </option > ';
                        $("#SizeCont").empty();
                        $("#SizeCont").attr("Disabled", false);
                        var StockType = $("#StockType").val();
                        for (var i = 0; i < data.length; i++) {
                            if (data[i].stockTypeID == StockType)
                                s += '<option value = "' + data[i].ID + '" > ' + data[i].SizeDesc + '</option > ';
                        }
                        $("#SizeCont").append(s);

                    }
                });
            })


            $("#SizeCont").change(function () {

                $.ajax({
                    type: "GET",
                    url: "/CustomerRental/getDepartment",
                    data: "{}",
                    success: function (data) {
                        $("#TransactionType").empty();
                        var s = '< option value ="ff">  </option > ';
                        for (var i = 0; i < data.length; i++) {
                            s += '<option value = "' + data[i].DepID + '" > ' + data[i].description + '</option > ';
                        }
                        $("#TransactionType").append(s);
                        $("#TransactionType").attr("Disabled", false);
                    }
                });

            })
      
            var Custtable = $('#tCustomer').DataTable({
                "ajax": {
                    "url": "/CustomerRental/GetCustomers",
                    "type": "GET",
                    "datatype": "json"
                },
                "columns": [
                    { "data": "space" },
                    { "data": "CustmerID" },
                    { "data": "CustID" },
                    { "data": "CustName" },
                    { "data": "CustSurname" },
                    { "data": "CustPhone" },
                    { "data": "CustEmail" }


                ],
                bFilter: false, bInfo: false,
                columnDefs: [{
                    orderable: false,
                    className: 'select-checkbox',
                    targets: 0
                }],

                select: {
                    style: 'os',
                    selector: 'td:first-child'
                },
                "pageLength": 5
            });
            $("#SearchCus").keyup(function () {
                var input, filter, table, tr, td, i, txtValue;
                input = document.getElementById("SearchCus");
                filter = input.value.toUpperCase();
                table = document.getElementById("tCustomer");
                tr = table.getElementsByTagName("tr");
                for (i = 0; i < tr.length; i++) {
                    td = tr[i].getElementsByTagName("td")[2];
                    if (td) {
                        txtValue = td.textContent || td.innerText;
                        if (txtValue.toUpperCase().indexOf(filter) > -1) {
                            tr[i].style.display = "";
                        } else {
                            tr[i].style.display = "none";
                        }
                    }
                }



            });
            /* Order Transactio*/

            $("#TransactionType").change(function () {
                var EstablishID = $("#Establishment").val();
                var InstituteID = $("#Institutes").val();
                var StockType = $("#StockType").val();
                var TransactionType = $("#TransactionType").val();
                $.ajax({
                    type: "GET",
                    url: "/CustomerRental/getPrice",
                    data: { 'EstablshID': EstablishID, 'InstID': InstituteID, 'stckType': StockType, 'Trans': TransactionType },
                    success: function (data) {
                        $("#PriceItem").attr("value", parseFloat(data).toFixed(2))

                    }
                });


            });

            $("#AddList").on("click", function () {
                var EstablishID = $("#Establishment").val();
                var InstituteID = $("#Institutes").val();
                var StockType = $("#StockType").val();
                var TransactionType = $("#TransactionType").val();
                var Price = $("#PriceItem").val()
                var Size = $("#SizeCont").val();



                if (Price == "") {

                    toastr.error("Please select the Transaction Type to retrieve the item price");

                }
                else {

                    var Colour = $("#ColourItems").val();

                    var Faculty = $("#Faculty").val();
                    var Quali = $("#QualType").val();

                    if (Faculty == "0" || Faculty == 0 || Faculty == "" || Faculty == null)
                        Quali = Colour;
                    var RentalAmount = $("#RentalAmount").val();
                    var RetailAmount = $("RetailAmount").val();

                    $.ajax({
                        type: "GET",
                        url: "/CustomerRental/setPrice",
                        dataType: "Json",
                        data: { 'Quali': Quali, 'EstablishID': EstablishID, 'Fac': Faculty, 'InstituteID': InstituteID, 'StockType': StockType, 'TransactionType': TransactionType, 'RentalAmount': RentalAmount, 'RetailAmount': RetailAmount, 'StockSize': Size },
                        success: function (data) {
                            if (data == "failed") {

                                toastr.error("Stock items fitting the criteria are not avaiable in our store!")
                            }
                            else {


                                $("#tRequested").empty();
                                $.getJSON('/Scripts/AddListItems.json', function (res) {
                                    var trHTML = '';
                                    var count = 1;
                                    $.each(res, function (i, item) {
                                        trHTML += '<tr><td class="Type">' + item.StockType + '</td><td>' + item.StockSize + '</td><td>' + item.StockColour + '</td><td id="Trans">' + item.TransactionType + '</td><td>' + parseFloat(item.RentalPrice).toFixed(2) + '</td><td class="border rounded-0" style = "color: rgb(206,210,213);" > <button class="btn btn-primary" id="Remove"  value="' + count + '"type = "button" style = "background-color: rgb(216,74,56);" > Remove</button ></td></tr > ';
                                        count++;
                                    });
                                    $("#tRequested").append(trHTML);
                                });
                                /* var TotalPrice = $("#TotalPrice").val();
                                 var CurrentPrice = $("#PriceItem").val();
                                 $.ajax({
                                     type: "GET",
                                     url: "/CustomerRental/getTotalPrice",
                                     dataType: "Json",
                                     data: { 'Price': TotalPrice, 'CurrentPrice': CurrentPrice },
                                     success: function (data) {
                                         $("#TotalPrice").attr("value", data);
                                     }
                                 });*/


                            }

                        }
                    });
                }
             
            });

            $("#DisplayOutFit").on("click", function () {

                var EstablishID = $("#Establishment").val();
                var InstituteID = $("#Institutes").val();

                $.ajax({
                    type: "GET",
                    url: "/CustomerRental/getStockType",
                    data: "{}",
                    success: function (data) {
                        var s = '< option value ="ff">  </option > ';
                        for (var i = 0; i < data.length; i++) {
                            if (data[i].EstaID == $("#Establishment").val())
                                s += '<option value = "' + data[i].StckTypeID + '" > ' + data[i].description + '</option > ';
                        }
                        $("#StockType").append(s);
                        $("#StockType").attr("Disabled", false);
                    }
                });


            })



            $("#AddToCart").click(function () {

                var barCode = $("#stockBarcode").val();
 
                $.ajax({
                    type: "GET",
                    url: "/CustomerRental/getStockItem",
                    dataType: "Json",
                    data: { 'barCode': barCode },
                    success: function (data) {
                        if (data == "Description Different") {
                            alert("Item did not match any item in the requested list");
                        }
                        else
                            if (data == "Item belongs to a different transaction") {
                                alert("Item belongs to a different transaction");
                            }
                            else {

                                var trHTML = '';

                                $.each(data, function (i, item) {
                                    trHTML += '<tr><td>' + item.Bar + '</td><td>' + item.StckTy + '</td><td>' + item.stckSize + '</td><td>' + item.StckCOlour + '</td><td>' + parseFloat(item.StckPrice).toFixed(2) + '</td> </tr>';
                                    if (item.TransType == "Retail")
                                        $("#RetailCartDetail").append(trHTML);
                                    else
                                        if (item.TransType == "Rental")
                                            $("#RentalCartDetail").append(trHTML);
                                });


                                $("#SubmitOrder").attr("disabled", false)
                            }


                    }

                });
            });

            $("#StockItem").click(function () {

                var barCode = $("#stockBarcode").val();

                $.ajax({
                    type: "GET",
                    url: "/CustomerRental/getStockItem",
                    dataType: "Json",
                    data: { 'barCode': barCode },
                    success: function (data) {
                        if (data == "Description Different") {
                            toastr.error("Item did not match any item in the requested list");
                        }
                        else
                            if (data == "Item belongs to a different transaction") {
                                toastr.error("Item belongs to a different transaction");
                            }
                            else {

                                var trHTML = '';
                                $.each(data, function (i, item) {

                                    trHTML += '<tr><td>' + item.Bar + '</td><td>' + item.StckTy + '</td><td>' + item.stckSize + '</td><td>' + item.StckCOlour + '</td><td>' + item.TransType + '</td></tr>';
                                });
                                $("#StockItemDetails").append(trHTML);
                            }


                    }

                });


            });



            var selectedData;
            $('#tCustomer').on('click', 'tr', function () {
                selectedData = Custtable.row(this).data();
                $("#Establishment").attr("Disabled", false);


            });
            $("#SubmitOrder").on("click", function () {

                if (selectedData != null) {

                    CustID = selectedData.CustmerID;
                    Name = selectedData.CustName + " " + selectedData.CustSurname;


                    Amount = $("#TotalPrice").val();
                    $.ajax({
                        type: "GET",
                        url: "/CustomerRental/PlaceOrder",
                        data: { 'CustID': CustID, 'Name': Name, 'Amount': Amount },
                        success: function (data) {
                            if (data == "failed") {
                                toastr.error("Please scan the items from the requested list");
                            }
                            toastr.success("Customer Order has Been Placed, Please ACCESS the generated report in the report folder");
                        }


                    });

                }
                else
                    toastr.error("Please scan the items from the requested list");
             



            });
            /*end*/
            $("#menu-toggle").click(function (e) {
                e.preventDefault();
                $("#wrapper").toggleClass("toggled");
            });
            $('.textInput').keyup(function () {
                $('span.error-keyup-2').remove();
                var inputVal = $(this).val();
                var characterReg = /^\s*[a-zA-Z,\s]+\s*$/;
                if (!characterReg.test(inputVal)) {
                    $(this).after('<span class="error error-keyup-2" style="background-color:red">Only Alphabets characters allowed.</span>');
                }
            });
            $('#PhoneNum').keyup(function () {
                $('span.error-keyup-1').hide();
                var inputVal = $(this).val();
                var numericReg = /^\d*[0-9](|.\d*[0-9]|,\d*[0-9])?$/;
                if (!numericReg.test(inputVal)) {
                    $(this).after('<span class="error error-keyup-1" style="background-color:red">Numeric characters only.</span>');
                }
            });
            $('#NextPhoneNum').keyup(function () {
                $('span.error-keyup-1').hide();
                var inputVal = $(this).val();
                var numericReg = /^\d*[0-9](|.\d*[0-9]|,\d*[0-9])?$/;
                if (!numericReg.test(inputVal)) {
                    $(this).after('<span class="error error-keyup-1" style="background-color:red">Numeric characters only.</span>');
                }
            });

            $("#AddCustomer").on('click', function () {

                var flag = true;
                $(".error error - keyup - 1").remove();
                $("span").remove();
                var CustID = $("#CustomerID").val();
                var CustName = $("#CustName").val();
                var PhoneNum = $("#PhoneNum").val();
                var CustSur = $("#CustSurn").val();
                var Address = $("#CustAddAddres").val();
                var Email = $("#CustEmail").val();
                var NextKinName = $("#NextName").val();
                var NextKinPhone = $("#NextPhoneNum").val();
                if (CustID.length == 0) {
                    $("#CustomerID").after('<span class="error error-keyup-1" style="background-color:red">Please Enter Customer ID.</span>');
                    flag = false;
                }
                if (CustName.length == 0) {
                    $("#CustName").after('<span class="error error-keyup-1" style="background-color:red">Missing Customer Name</span>');
                    flag = false;
                }
                if (PhoneNum.length == 0) {
                    $("#PhoneNum").after('<span class="error error-keyup-1" style="background-color:red">Field left empty.</span>');
                    flag = false;
                }
                if ($("#CustSurn").val().length == 0) {
                    $("#CustSurn").after('<span class="error error-keyup-1" style="background-color:red">Field left empty</span>');
                    flag = false;
                }
                if (Address.length == 0) {
                    $("#CustAddAddres").after('<span class="error error-keyup-1" style="background-color:red">Field left empty</span>');
                    flag = false;
                }
                if (Email.length == 0) {
                    $("#CustEmail").after('<span class="error error-keyup-1" style="background-color:red">Field left empty</span>');
                    flag = false;
                }
                if (NextKinName.length == 0) {
                    $("#NextName").after('<span class="error error-keyup-1" style="background-color:red">Numeric characters only.</span>');
                    flag = false;
                }
                if (NextKinPhone.length == 0) {
                    $("#NextPhoneNum").after('<span class="error error-keyup-1" style="background-color:red">Field left empty</span>');
                    flag = false;
                }

                if (flag) {
                    var form = $("#CustomerAddForm");
                    $.ajax({
                        type: "Post",
                        url: "/CustomerRental/SaveCustomer",
                        contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                        data: form.serialize(),
                        success: function (data) {

                            toastr.warning("Search the customer and select the customer");
                            table.ajax.reload();
                        }
                    });
                }
                else
                    toastr.error("Please complete the outstanding fields!");




            });
            var table = $('#dt-basic-checkbox').DataTable({
                "ajax": {
                    "url": "/CustomerRental/loadData",
                    "type": "GET",
                    "datatype": "json"
                },
                "columns": [
                    { "data": "space" },
                    { "data": "ID" },
                    { "data": "StockType" },
                    { "data": "Size" },
                    { "data": "ColourName" }

                ],
                columnDefs: [{
                    orderable: false,
                    className: 'select-checkbox',
                    targets: 0
                }],
                select: {
                    style: 'os',
                    selector: 'td:first-child'
                },
                "pageLength": 5
            });

            $("#SaveChanges").on("click", function () {


                var barCode = $("#stockBarcode").val();

                $.ajax({
                    type: "GET",
                    url: "/CustomerRental/SaveItem",
                    dataType: "Json",
                    data: { 'barCode': barCode },
                    success: function (data) {
                        if (data == "Description Different") {
                            toastr.error("Item did not match any item in the requested list");
                        }
                        else
                            if (data == "Item belongs to a different transaction") {
                                toastr.error("Item belongs to a different transaction");
                            }
                            else {

                                var trHTML = '';
                                $.each(data, function (i, item) {
                                    trHTML += '<tr><td>' + item.Bar + '</td><td>' + item.StckTy + '</td><td>' + item.stckSize + '</td><td>' + item.StckCOlour + '</td><td>' + parseFloat(item.StckPrice).toFixed(2) + '</td> </tr>';
                                    if (item.TransType == "Retail")
                                        $("#RetailCartDetail").append(trHTML);
                                    else
                                        if (item.TransType == "Rental")
                                            $("#RentalCartDetail").append(trHTML);
                                });
                                $("#CartDetails").append(trHTML);
        
                                $('#StockModel').modal('hide');
                                toastr.success("Item has been placed to the cart");
                            }


                    }

                });
            });

            $('#DisplayItem').on('click', function (e) {
                var barCode = $("#stockBarcode").val();

                $.ajax({
                    type: "GET",
                    url: "/CustomerRental/getStockItem",
                    dataType: "Json",
                    data: { 'barCode': barCode },
                    success: function (data) {
                        if (data == "Description Different") {
                            toastr.error("Item did not match any item in the requested list");
                        }
                        else
                            if (data == "Item belongs to a different transaction") {
                                toastr.error("Item belongs to a different transaction");
                            }
                            else {

                                var trHTML = '';

                                $("#StockItemDetails").empty();
                                $.each(data, function (i, item) {

                                    trHTML += '<tr><td>' + item.Bar + '</td><td>' + item.StckTy + '</td><td>' + item.stckSize + '</td><td>' + item.StckCOlour + '</td><td>' + item.TransType + '</td></tr>';
                                });
                                $("#StockItemDetails").append(trHTML);
                                $('#StockModel').modal('show');

                            }


                    }

                });


                e.preventDefault();
            });

            $("#Out *").attr("disabled", true);

            $("#Cancel").on("click", function () {
                alert("deletomg")
                $.getJSON('/Scripts/AddListItems.json', function (res) {
                    var trHTML = '';
                    var count = 1;
                    var TransType = "";
                    var DescriptID = "";
                    var bcount = 0;
                    $('#WarningModal').modal('hide');
                    $.each(res, function (i, item) {

                        if (count == Delete) {
                            TransType = item.TransactionType;
                            DescriptID = item.DescriptionID;
                            bcount = count;
                        }
                        count++;
                    });

                    if (bcount == Delete) {
                        $.ajax({
                            type: "GET",
                            url: "/CustomerRental/DeleteDescript",
                            dataType: "Json",
                            data: { 'DescriptID': DescriptID, 'TransType': TransType },
                            success: function (data) {
                                if (data == "Success") {
                                    $("#tRequested").empty();
                                    $.getJSON('/Scripts/AddListItems.json', function (res) {
                                        var trHTML = '';
                                        var count = 1;
                                        $.each(res, function (i, item) {
                                            trHTML += '<tr><td class="Type">' + item.StockType + '</td><td>' + item.StockSize + '</td><td>' + item.StockColour + '</td><td id="Trans">' + item.TransactionType + '</td><td>' + parseFloat(item.RentalPrice).toFixed(2) + '</td><td class="border rounded-0" style = "color: rgb(206,210,213);" > <button class="btn btn-primary" id="Remove"  value="' + count + '"type = "button" style = "background-color: rgb(216,74,56);" > Remove</button ></td></tr > ';
                                            count++;
                                        });
                                        $("#tRequested").append(trHTML);
                                    });
                                }
                                else
                                toastr.error("Item has already been linked to a stock item");
                            }
                        });
                    }
                    else
                        toastr.error("Item not found in the list");
                    
                });


            });
            $("#CancelTransaction").on("click", function () {
                $('#centralModal').modal('hide');
                window.location.href = "Create";

            });

        });
        var Delete = "";
        $(document).on("click", "#Remove", function () {
            
            Delete = $(this).val();
            $('#WarningModal').modal('show');


        })
       
  

    </script>
